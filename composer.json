{
    "name": "tcoch/netdoc",
    "license": "MIT",
    "description": "Easy parsing of networking and infrastructure components",
    "require": {
        "php": "^8.3",
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tcoch\\Netdoc\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68",
        "phpstan/phpstan": "^2",
    }
}
